<core:FragmentDefinition xmlns:table="sap.ui.table"
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>textValueHelpDialog}"
		ok=".onValueHelpOkPressAufpl"
		cancel=".onExitAufpl"
		key="aufpl"
		descriptionKey="Name"
		supportMultiselect="false"
	>
		<filterBar>
			<fb:FilterBar search="onFilterBarSearchAufpl">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="aufpl"
						label="{i18n>textLabelOperation}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="aufpl" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>